#defaultVisualComm {
	background-image: url('../../../../library/images/visualComm.bg.png');
	background-repeat: repeat-x;
	height: 438px;
	width: 100%;
}

#contentVisualComm {
	margin-left: auto;
	margin-right: auto;
	width: 950px;
	margin-top: 20px;
	padding: 20px;
}

#contentNewsFront {
	margin-left: auto;
	margin-right: auto;
	width: 1000px;
	padding-top: 6px;
	padding-bottom: 5px;
}

#listVisualComm {
	height: 100px;
	border-bottom-style: solid;
	border-bottom-color: #b4b4b4;
	border-bottom-width: 1px;
	-moz-box-shadow: 0px 0px 1px #fff;
	-webkit-box-shadow: 0px 0px 1px #fff;
	box-shadow: 0px 0px 1px #fff;
}

#newsFront {
	height: 25px;
	border-bottom-style: solid;
	border-bottom-color: #b4b4b4;
	border-bottom-width: 1px;
	-moz-box-shadow: 0px 0px 1px #fff;
	-webkit-box-shadow: 0px 0px 1px #fff;
	box-shadow: 0px 0px 1px #fff;
}

#footer {
	background-color: #323232;
	height: 172px;
}

.boldNewsFront {
	font-weight: bold;
	padding-right: 10px;
}

.grayNewsFront {
	color: #183c67;
}